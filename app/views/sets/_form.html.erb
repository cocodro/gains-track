<% form_for [exercise, set] do |f| %>
  <div class="form-group">
    <%= f.label :order %>
    <%= f.number_field :order, autofocus: true, class: "form-control", placeholder: "Set # ?" %>
  </div>
  <div class="form-group">
    <%= f.label :reps %>
    <%= f.number_field :reps, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.submit "Save Set", class: 'btn btn-success' %>
  </div>
<% end %>
